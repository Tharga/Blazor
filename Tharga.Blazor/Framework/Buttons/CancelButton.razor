<StandardButton Text="@Text" Icon="cancel" Click="@Click" Type="@StandardButton.EButtonType.Normal" Variant="Variant.Outlined" Enabled="@Enabled" />

@code {
    [Parameter]
    public string Text { get; set; } = "Cancel";

    [Parameter]
    public Func<Task> Click { get; set; }

    [Parameter]
    public bool Enabled { get; set; } = true;
}